<script>
window.addEventListener("load", function() {
      var nodes = document.querySelectorAll(".
        blur > * ");
        for (var i = 0; i < nodes.lenght; i++) {
          nodes[i].style.animationDelay = (i * 3) + "s";
        }

      });
      </script>
